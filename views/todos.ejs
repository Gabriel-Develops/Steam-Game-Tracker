    <main class="dashboardMain">
        <div class="dashboardDiv1">
        <h1><a href="https://store.steampowered.com/app/<%= appID %>" target="_blank"><%= gameName.split('_').join(' ') %></a><h1/>
        <h1>Achievements: <%= userAchievements %> / <%= totalAchievements %> <h1/>
        
            <h1>Todos</h1>
                <ul>
                    <% todos.forEach( el => { %>
                        <li class='todoItem' data-id='<%= el._id %>'>
                            <span class='<%= el.completed === true ? 'completed' : 'not'%>'><%= el.todo %></span>
                            <span class='del'> Delete </span>
                        </li>
                    <% }) %>    
                </ul>
            <h2><%= user.steamUserName %> has <%= left %> things left to do in <%= gameName.split('_').join(" ") %>.</h2>
        </div>

        <div class="dashboardDiv2">
            <form action="/todos/createTodo/<%= user.steamID %>/<%= appID %>/<%= gameName %>" method='POST'>
                <input type="text" placeholder="Enter Todo Item" name='todoItem'>
                <input type="submit" class="inputSubmit">
            </form>

        </div>

        
    </main>
<script src="/js/todos.js"></script>